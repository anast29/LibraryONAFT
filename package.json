{
  "name": "library-onaft",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-locale:en": "ng build --prod=true  --lazyModules --outputHashing=none --serviceWorker=true --i18n-locale en --i18n-format xlf --i18n-file src/translate/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:ua": "ng build --prod=true  --lazyModules --outputHashing=none --serviceWorker=true --i18n-locale ua --i18n-format xlf --i18n-file src/translate/messages.ua.xlf --output-path=dist --baseHref /",
    "build-locale:ru": "ng build --prod=true  --lazyModules --outputHashing=none --serviceWorker=true --i18n-locale ru --i18n-format xlf --i18n-file src/translate/messages.ru.xlf --output-path=dist/ru --baseHref /ru/",
    "build": "npm run build-locale:ua &&  npm run build-locale:en && npm run build-locale:ru",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cli": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "^7.1.0",
    "@angular/service-worker": "^7.1.0",
    "angular-router-loader": "^0.8.5",
    "bootstrap": "^4.3.1",
    "bootstrap-dropdown-hover": "^4.2.0",
    "bootstrap-hover-dropdown": "^2.2.1",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "i18n": "^0.8.3",
    "jquery": "^3.4.0",
    "npm": "^6.9.0",
    "popper": "^1.0.1",
    "popper.js": "^1.14.6",
    "rxjs": "6.4.0",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.2.13",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.29",
    "@types/node": "~11.13.6",
    "codelyzer": "~5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-page-scroll": "^6.0.0-beta.1",
    "ngx-page-scroll-core": "6.0.0-beta.0",
    "protractor": "^6.0.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "^3.1.2",
    "zone.js": "0.8.26"
  }
}
